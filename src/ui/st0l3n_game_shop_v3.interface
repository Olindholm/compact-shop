					!StringEquals(_storeCompCurEntity{itemIndex=0}, _storeCompOldEntity{itemIndex=0}),
					Split(
						FadeOut(200 * _shopFlag_animSpeedMult),
						SleepWidget(200 * _shopFlag_animSpeedMult, 'DoEvent(0);')
					),
					DoEvent(0)
				);
			"
			onevent0="
